#include <stdio.h> 

int main() 
{ 
int i = 0, k = 0, zed; //инициализация переменных-индексов 
int array[] = { 4,12,5,6,0,14,1 }; //инициализация массива,задаём его элементы 
int n = sizeof(array) / sizeof(int); //вычисляем размер массива 
while (i < n - 1) //задаём предусловие 
{ 
while (k < n - i - 1) //задаём предусловие 
{ 
if (array[k] > array[k + 1]) //задаем условие цикла 
{ 
zed = array[k]; //Меняем местами 
array[k] = array[k + 1]; //соседние элементы 
array[k + 1] = zed; //если элемент с меньшим индексом больше последующего элемента 
} 
k++; //переходим к следующему элементу 
} 
i++; //переходим к следующему элементу 
} 
for (int i= 0; i < n; i++) 
{ 
printf("%d", array[i]); 
printf(" "); 
} 
return 0; 
}